class Solution {
  public:
    void printTriangle(int n) {
        // code here
          
        int num=65;
        for(int i=0;i<n;i++)
        {
            for(int j=0;j<=i;j++)
            {
                cout<<(char)num;
            }
            num++;
            cout<<endl;
        }
    }
};


